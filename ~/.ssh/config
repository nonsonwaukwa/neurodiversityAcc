# Personal GitHub account (nonsonwaukwa)
Host github-personal
   HostName github.com
   User git
   IdentityFile ~/.ssh/id_ed25519_personal
   IdentitiesOnly yes

Host github.com
   HostName github.com
   User git
   IdentityFile ~/.ssh/id_ed25519_personal
   IdentitiesOnly yes

# Orecreates GitHub account
Host github-ore    
   HostName github.com
   User git
   IdentityFile ~/.ssh/id_ed25519_orecreates
   IdentitiesOnly yes

Host github.com-orecreates
  HostName github.com
  User git
  IdentityFile ~/.ssh/id_ed25519_orecreates
  IdentitiesOnly yes 